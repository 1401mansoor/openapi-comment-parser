{
  "name": "openapi-comment-parser",
  "version": "0.0.14",
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT",
  "description": "JSDoc Comments for the OpenApi Specification",
  "homepage": "https://github.com/bee-travels/openapi-comment-parser",
  "repository": {
    "type": "git",
    "url": "git://github.com/bee-travels/openapi-comment-parser.git"
  },
  "bugs": {
    "url": "https://github.com/bee-travels/openapi-comment-parser/issues"
  },
  "keywords": [
    "openapi",
    "swagger",
    "jsdoc",
    "comment",
    "api",
    "express"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/openapi-comment-parser.js",
  "module": "dist/openapi-comment-parser.esm.js",
  "types": "openapi-comment-parser.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run build",
    "test": " mocha --timeout 10000 --exit",
    "start": "nodemon -e yaml,yml,js,mjs,json example/src/app.js",
    "build": "run-s clean bundle",
    "clean": "rimraf dist",
    "bundle": "npm run xxx && rollup -c && cp src/index.d.ts dist/openapi-comment-parser.d.ts",
    "xxx": "tsc -p tsconfig.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-sucrase": "^3.0.1",
    "@rollup/plugin-typescript": "^4.1.1",
    "body-parser": "1.19.0",
    "chai": "4.2.0",
    "chai-jest-snapshot": "2.0.0",
    "comment-parser": "^0.7.4",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "express": "4.17.1",
    "globby": "^11.0.0",
    "js-yaml": "^3.13.1",
    "mocha": "7.1.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "supertest": "4.0.2",
    "swagger-parser": "^9.0.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.2"
  }
}
